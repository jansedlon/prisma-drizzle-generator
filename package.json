{
  "name": "prisma-drizzle-generator",
  "version": "1.0.0",
  "description": "A Prisma generator that converts Prisma schemas to Drizzle ORM schemas with comprehensive relation support",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "prisma-drizzle-generator": "dist/index.js"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:e2e": "vitest run tests/e2e",
    "test:types": "tsd",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:types && npm run test:e2e",
    "test:ci": "vitest run --coverage --reporter=verbose",
    "prisma": "prisma",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",
    "docker:postgres": "docker run --name drizzle-test-postgres -e POSTGRES_PASSWORD=testpass -e POSTGRES_DB=testdb -p 5432:5432 -d postgres:15",
    "docker:mysql": "docker run --name drizzle-test-mysql -e MYSQL_ROOT_PASSWORD=testpass -e MYSQL_DATABASE=testdb -p 3306:3306 -d mysql:8",
    "docker:cleanup": "docker stop drizzle-test-postgres drizzle-test-mysql && docker rm drizzle-test-postgres drizzle-test-mysql",
    "benchmark": "vitest run tests/performance --reporter=verbose",
    "lint": "eslint src tests --ext .ts",
    "lint:fix": "eslint src tests --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist coverage",
    "pretest": "npm run build",
    "prepublishOnly": "npm run clean && npm run build && npm run test:ci"
  },
  "dependencies": {
    "@prisma/generator-helper": "^5.7.1",
    "drizzle-orm": "^0.29.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/tmp": "^0.2.6",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "tsdown": "^0.2.15",
    
    "vitest": "^1.1.0",
    "@vitest/ui": "^1.1.0",
    "@vitest/coverage-v8": "^1.1.0",
    
    "tsd": "^0.30.0",
    
    "@testcontainers/postgresql": "^10.4.0",
    "@testcontainers/mysql": "^10.4.0",
    "testcontainers": "^10.4.0",
    
    "drizzle-kit": "^0.20.7",
    "postgres": "^3.4.3",
    "mysql2": "^3.6.5",
    "better-sqlite3": "^9.2.2",
    "@types/better-sqlite3": "^7.6.8",
    
    "rimraf": "^5.0.5",
    "@types/rimraf": "^4.0.5",
    "tmp": "^0.2.1",
    "fs-extra": "^11.2.0",
    "@types/fs-extra": "^11.0.4",
    
    "docker-compose": "^0.24.6",
    "@types/docker-compose": "^0.0.34"
  },
  "peerDependencies": {
    "prisma": "^5.0.0",
    "drizzle-orm": "^0.29.0"
  },
  "keywords": [
    "prisma",
    "drizzle",
    "orm",
    "generator",
    "database",
    "typescript",
    "relations",
    "schema"
  ],
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-org/prisma-drizzle-generator.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/prisma-drizzle-generator/issues"
  },
  "homepage": "https://github.com/your-org/prisma-drizzle-generator#readme",
  "license": "MIT",
  "author": "Your Name <your.email@example.com>",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/your-username"
  }
}
